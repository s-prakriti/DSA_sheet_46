# Node class
class Node:
    def __init__(self, new_data):
        self.data = new_data
        self.next = None


# Queue class
class myQueue:

    def __init__(self):
        self.front = None  # Points to the front node
        self.rear = None   # Points to the rear node
        self.count = 0     # Keeps track of size

    def isEmpty(self):
        return self.front is None

    def enqueue(self, x):
        new_node = Node(x)
        if self.rear is None:
            # First element being added
            self.front = self.rear = new_node
        else:
            self.rear.next = new_node
            self.rear = new_node
        self.count += 1

    def dequeue(self):
        if self.isEmpty():
            return  # Do nothing if queue is empty
        self.front = self.front.next
        if self.front is None:
            # Queue is now empty, so reset rear as well
            self.rear = None
        self.count -= 1

    def getFront(self):
        if self.isEmpty():
            return -1
        return self.front.data

    def size(self):
        return self.count
